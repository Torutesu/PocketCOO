# PersonalOS Environment Variables

# OpenAI API
OPENAI_API_KEY=your_openai_api_key_here

# Optional: OpenAI settings
# OPENAI_MODEL=gpt-4o-mini
# OPENAI_BASE_URL=https://api.openai.com/v1

# Optional: Backup (OpenAI-compatible)
# OPENAI_API_KEY_BACKUP=your_backup_openai_compatible_api_key_here
# OPENAI_MODEL_BACKUP=gpt-4o-mini
# OPENAI_BASE_URL_BACKUP=https://api.openai.com/v1

# Alternative: Claude API
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Optional: Claude settings
# CHAT_LLM_PROVIDER=anthropic
# ANTHROPIC_MODEL=claude-3-5-sonnet-latest
# ANTHROPIC_BASE_URL=https://api.anthropic.com
# ANTHROPIC_VERSION=2023-06-01
# ANTHROPIC_MAX_TOKENS=1024

# Optional: Claude backup
# ANTHROPIC_API_KEY_BACKUP=your_anthropic_api_key_backup_here
# ANTHROPIC_MODEL_BACKUP=claude-3-5-sonnet-latest
# ANTHROPIC_BASE_URL_BACKUP=https://api.anthropic.com

# memU Configuration
MEMU_CONFIG_PATH=./config/memu_config.json

# MemuU (managed memory service)
# Set this in your local .env or in the server environment. Do not commit real keys.
MEMUU_API_KEY=your_memuu_api_key_here
MEMUU_BASE_URL=https://api.memu.so
MEMUU_AGENT_ID=personalos
MEMUU_AGENT_NAME=PersonalOS

# Database
DATABASE_URL=sqlite:///./personalos.db

# Backend
BACKEND_HOST=0.0.0.0
BACKEND_PORT=8000

# API Key auth (optional). If set, backend requires x-api-key header.
API_KEY=your_backend_api_key_here

# Frontend
NEXT_PUBLIC_API_URL=http://localhost:8000
NEXT_PUBLIC_API_KEY=your_backend_api_key_here

# CORS
CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# Security
SECRET_KEY=your_secret_key_here_change_in_production
ENCRYPTION_KEY=your_encryption_key_here

# Qdrant (Vector Database)
QDRANT_HOST=localhost
QDRANT_PORT=6333

# Logging
LOG_LEVEL=INFO

# Development
DEBUG=True
